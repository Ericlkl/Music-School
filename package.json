{
  "name": "music-school-server",
  "version": "1.0.0",
  "description": "MERN Stack to build Pineland Music School",
  "main": "index.js",
  "engines": {
    "node": "11.3.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently 'npm run server' 'npm run client' ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ericlkl/Music-School.git"
  },
  "author": "KA LONG LEE(Eric)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ericlkl/Music-School/issues"
  },
  "homepage": "https://github.com/Ericlkl/Music-School#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.4",
    "express-validator": "^6.0.1",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.0",
    "moment": "^2.24.0",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "sharp": "^0.22.1",
    "validator": "^10.11.0"
  }
}
